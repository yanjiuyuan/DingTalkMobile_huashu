/* --------修改----------*/

.form-row {
	min-height: 90rpx;
  display: flex;
  align-items: center;
  padding: 6rpx 10rpx;
  margin: 20rpx 0;
  background-color: #fff;
}

.form-row-label {
  width: 260rpx;
  font-size: 34rpx;
  margin-left: 10rpx;
  color: #000;
  background-color: #fff;
  
}



.form-row-content {
  flex: 1;
  font-size: 34rpx;
  text-align: right;

} 


.form-line {
  height: 1px;
  background-color: #ddd;
  margin-left: 30rpx;
}

.input{
  background-color:#fff;
  width:95%;
}



